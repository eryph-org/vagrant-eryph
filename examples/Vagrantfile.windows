# Windows Server catlet example - using new Ruby-style syntax
Vagrant.configure("2") do |config|
  config.vm.provider :eryph do |eryph|
    # Minimal Windows configuration
    eryph.parent = "dbosoft/winsrv2022-standard/latest"
    eryph.configuration_name = "zero"  # Use 'zero' configuration
  end
  
  config.vm.guest = :windows
end